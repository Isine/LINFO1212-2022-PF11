<!DOCTYPE html>
<html lang="FR">

<head>
  <meta charset="utf-8">
  <title>SyllaExchange</title>
  <link rel="stylesheet" href="style.css">
  <link rel="shortcut icon" href="images/logo.icon">

  <header>
    <a href="/">
      <img class="logo" src="images/logo.png" alt="SyllaExchange-Logo">
    </a>

    <h1 class="title"><a href="/" id="title_link">SyllaExchange</a></h1>

    <input type="button" value=<%=user %> id="connexion" onclick="location.href='/login';">

  </header>
</head>

<body>
  <hr>

  <div class="catalogueV">
    <img class="arrow" src="/private/images/FlêcheG.png" alt="">

    <% articleList.forEach(function(article){ %>
      <div class="itemsV">
        <p class="Book_name">
          <%= article.title %>
        </p>
        <img src=<%=article.image %> alt="Syllabus-image" class="booksV">
        <div class="info">
          <h2>
            <%= article.price %> €
          </h2>
          <h2>
            <%= article.desc %>
          </h2>
          <h2> vendu par <%= article.seller %>
          </h2>
        </div>
      </div>
      <% }) %>
  </div>


</body>

</html>